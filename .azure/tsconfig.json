{
    "extends": "@tsconfig/node16/tsconfig.json",
    "compilerOptions": {
        "declaration": false,
        "sourceMap": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "baseUrl": ".",
        "allowSyntheticDefaultImports": true,
    }
}
