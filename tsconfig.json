{
    "extends": "@tsconfig/node10/tsconfig.json",
    "compilerOptions": {
        "declaration": false,
        "sourceMap": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "baseUrl": "./source",
        "allowSyntheticDefaultImports": true,
    }
}
